<view class="page">

  <view class="header">
    <text class="h2">
      Reservations
    </text>
  </view>

  <view class="sliding-images">
    <swiper class="slider" display-multiple-items="6">
      <block wx:for="{{months}}">
        <swiper-item class="swipe-block">
          <view class="sliding-months">
            <text class="p">{{item}}</text>
          </view>
        </swiper-item>
      </block>
    </swiper>
  </view>

  <view class="cards-container">

    <view class="card">

      <view class="user">
        <image />
      </view>

      <view class="item">
      </view>

      <view class="response">
        <view class="accept-tab">
          <text class="p">Accept</text>
        </view>

        <view class="reject-tab">
          <text class="p">Decline</text>
        </view>
      </view>

    </view>

  </view>



</view>
